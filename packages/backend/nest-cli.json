{
  "$schema": "https://json.schemastore.org/nest-cli",
  "collection": "@nestjs/schematics",
  "sourceRoot": "src",
  "compilerOptions": {
    "assets": [{
      "include": "assets/*.pdf", "outDir": "dist/src", "watchAssets": true
    }, {
      "include": "assets/*.ttf", "outDir": "dist/src", "watchAssets": true
    }],
    "deleteOutDir": true
  }
}
